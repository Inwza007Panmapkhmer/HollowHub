--resize

local frame = Frame
local UIS = game:GetService("UserInputService")
local resizing = false
local dragging = false
local offset = Vector2.new()
local resizeOffset = Vector2.new()



frame.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 and input.Position.X < (frame.AbsolutePosition.X + frame.AbsoluteSize.X - 10) and input.Position.Y < (frame.AbsolutePosition.Y + frame.AbsoluteSize.Y - 10) then
		dragging = true
		offset = Vector2.new(input.Position.X, input.Position.Y) - frame.AbsolutePosition
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	elseif input.UserInputType == Enum.UserInputType.MouseButton1 and input.Position.X > (frame.AbsolutePosition.X + frame.AbsoluteSize.X - 10) and input.Position.Y > (frame.AbsolutePosition.Y + frame.AbsoluteSize.Y - 10) then
		resizing = true
		resizeOffset = Vector2.new(input.Position.X, input.Position.Y) - (frame.AbsolutePosition + frame.AbsoluteSize)
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				resizing = false
			end
		end)
	end
end)

UIS.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement then
		if dragging then
			frame.Position = UDim2.new(0, input.Position.X - offset.X, 0, input.Position.Y - offset.Y)
		elseif resizing then
			local newSize = Vector2.new(input.Position.X, input.Position.Y) - frame.AbsolutePosition - resizeOffset
			frame.Size = UDim2.new(0, math.max(newSize.X, 100), 0, math.max(newSize.Y, 100))
			AboutFrame.Size = UDim2.new(0, math.max(newSize.X, 100), 0, math.max(newSize.Y, 100)) -- Minimum Size
			AnnoyingFrame.Size = UDim2.new(0, math.max(newSize.X, 100), 0, math.max(newSize.Y, 100)) -- Minimum Size
			ForceFrame.Size = UDim2.new(0, math.max(newSize.X, 100), 0, math.max(newSize.Y, 100)) -- Minimum Size
			PunchFrame.Size = UDim2.new(0, math.max(newSize.X, 100), 0, math.max(newSize.Y, 100)) -- Minimum Size
			SignFrame.Size = UDim2.new(0, math.max(newSize.X, 100), 0, math.max(newSize.Y, 100)) -- Minimum Size
		end
	end
end)

UIS.InputEnded:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		dragging = false
		resizing = false
	end
end)


--Button


Annoying.MouseButton1Click:Connect(function()
	AnnoyingFrame.Visible = true
	ForceFrame.Visible = false
	PunchFrame.Visible = false
	SignFrame.Visible = false
	AboutFrame.Visible = false
	TweenService:Create(UIGradient_4, TweenInfo.new(0.5, Enum.EasingStyle.Sine, Enum.EasingDirection.Out, 0, false, 0), {
		Offset = Vector2.new(-1, 0)
	}):Play()
	TweenService:Create(UIGradient_5, TweenInfo.new(0.5, Enum.EasingStyle.Sine, Enum.EasingDirection.Out, 0, false, 0), {
		Offset = Vector2.new(0, 0)
	}):Play()
	TweenService:Create(UIGradient_2, TweenInfo.new(0.5, Enum.EasingStyle.Sine, Enum.EasingDirection.Out, 0, false, 0), {
		Offset = Vector2.new(0, 0)
	}):Play()
	TweenService:Create(UIGradient_3, TweenInfo.new(0.5, Enum.EasingStyle.Sine, Enum.EasingDirection.Out, 0, false, 0), {
		Offset = Vector2.new(0, 0)
	}):Play()
end)

