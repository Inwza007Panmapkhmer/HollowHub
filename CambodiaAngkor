		Offset = Vector2.new(0, 0)
	}):Play()
	TweenService:Create(UIGradient_5, TweenInfo.new(0.5, Enum.EasingStyle.Sine, Enum.EasingDirection.Out, 0, false, 0), {
		Offset = Vector2.new(0, 0)
	}):Play()
	TweenService:Create(UIGradient_2, TweenInfo.new(0.5, Enum.EasingStyle.Sine, Enum.EasingDirection.Out, 0, false, 0), {
		Offset = Vector2.new(-1, 0)
	}):Play()
	TweenService:Create(UIGradient_3, TweenInfo.new(0.5, Enum.EasingStyle.Sine, Enum.EasingDirection.Out, 0, false, 0), {
		Offset = Vector2.new(0, 0)
	}):Play()
end)

Sign.MouseButton1Click:Connect(function()
	AnnoyingFrame.Visible = false
	ForceFrame.Visible = false
	PunchFrame.Visible = false
	SignFrame.Visible = true
	AboutFrame.Visible = false
	TweenService:Create(UIGradient_4, TweenInfo.new(0.5, Enum.EasingStyle.Sine, Enum.EasingDirection.Out, 0, false, 0), {
		Offset = Vector2.new(0, 0)
	}):Play()
	TweenService:Create(UIGradient_5, TweenInfo.new(0.5, Enum.EasingStyle.Sine, Enum.EasingDirection.Out, 0, false, 0), {
		Offset = Vector2.new(0, 0)
	}):Play()
	TweenService:Create(UIGradient_2, TweenInfo.new(0.5, Enum.EasingStyle.Sine, Enum.EasingDirection.Out, 0, false, 0), {
		Offset = Vector2.new(0, 0)
	}):Play()
	TweenService:Create(UIGradient_3, TweenInfo.new(0.5, Enum.EasingStyle.Sine, Enum.EasingDirection.Out, 0, false, 0), {
		Offset = Vector2.new(-1, 0)
	}):Play()
end)

Logo.MouseButton1Click:Connect(function()
	AnnoyingFrame.Visible = false
	ForceFrame.Visible = false
	PunchFrame.Visible = false
	SignFrame.Visible = false
	AboutFrame.Visible = true
end)
--SignFuncs

Button.MouseButton1Click:Connect(function()
	local player = game.Players:FindFirstChild(TextBox.Text)
	if player then
		local char = player.Character
		if char then
			if char:FindFirstChild("Sign") then
				char.Sign.UpdateSign:FireServer(Message.Text)
			end
		end
	end
end)

Button_2.MouseButton1Click:Connect(function()
	for _, player in ipairs(game.Players:GetChildren()) do
		local character = player.Character
		if character and character:FindFirstChild("Sign") then
			character.Sign.UpdateSign:FireServer(Message_2.Text)
		end
	end
end)

Button_3.MouseButton1Click:Connect(function()
	task.spawn(function()
		while wait(0.001) do
			for _, player in ipairs(game.Players:GetChildren()) do
				local character = player.Character
				if character and character:FindFirstChild("Sign") then
					character.Sign.UpdateSign:FireServer(Message_3.Text)
				end
			end
		end
	end)
end)

--ForceFuncs

Button_4.MouseButton1Click:Connect(function()
	local plr = TextBox_2.Text
	local args = {
		[1] = "Hug",
		[2] = game:GetService("Players"):WaitForChild(TextBox_2)
	}

	game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("HugEvents"):FireServer(unpack(args))
end)

Button_5.MouseButton1Click:Connect(function()
	local plr = TextBox_3.Text
	for i = 1, 10 do 
		local args = {
			[1] = "Hug",
			[2] = game:GetService("Players"):FindFirstChild(plr)
		}

		if args[2] then
			game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("HugEvents"):FireServer(unpack(args))
		end
	end

	local args = {
		[1] = "Stop",
		[2] = game:GetService("Players"):FindFirstChild(plr)
	}

	if args[2] then
		game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("HugEvents"):FireServer(unpack(args))
	end
end)

--AnnoyingFrame

Button_6.MouseButton1Click:Connect(function()
	local plr = TextBox_4.Text
	while wait(0.001) do
		local args = {
			[1] = "Ask",
			[2] = game:GetService("Players"):WaitForChild(plr)
		}

		game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("HugEvents"):FireServer(unpack(args))
	end
end)

Button_7.MouseButton1Click:Connect(function()
	while wait(0.001) do	
		for _, player in ipairs(game.Players:GetChildren()) do
			local args = {
				[1] = "Ask",
				[2] = game:GetService("Players"):WaitForChild(player)
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("HugEvents"):FireServer(unpack(args))
		end
	end
end)

--Punch

Button_8.MouseButton1Click:Connect(function()
	game.Players.LocalPlayer.Character.CombatClient.Enabled = true
end)
